<odoo>
    <record id="group_encounter_admin" model="res.groups">
        <field name="name">Encounter Admin</field>
        <field name="category_id" ref="base.module_category_hidden"/>
    </record>

    <record id="group_encounter_user" model="res.groups">
        <field name="name">Encounter User</field>
        <field name="category_id" ref="base.module_category_hidden"/>
    </record>

    <record id="access_encounter_admin" model="ir.model.access">
        <field name="name">access_encounter_admin</field>
        <field name="model_id" ref="model_dudoxx_encounter"/>
        <field name="group_id" ref="dudoxx_encounter.group_encounter_admin"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>

    <record id="access_encounter_user" model="ir.model.access">
        <field name="name">access_encounter_user</field>
        <field name="model_id" ref="model_dudoxx_encounter"/>
        <field name="group_id" ref="dudoxx_encounter.group_encounter_user"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="0"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="0"/>
    </record>
</odoo>
